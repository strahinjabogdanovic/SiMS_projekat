<Page x:Class="ProjekatSIMS.Upravnik.View.SveInfoPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ProjekatSIMS.Upravnik.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SveInfoPage" Height="675.2" Width="424">

    <Grid>
        <Label x:Name="label" Content="Ime" HorizontalAlignment="Left" Margin="50,50,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Oznaka" HorizontalAlignment="Left" Margin="50,120,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="Namena" HorizontalAlignment="Left" Margin="50,190,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label3" Content="Oprema" HorizontalAlignment="Left" Margin="50,260,0,0" VerticalAlignment="Top"/>
        <TextBlock Text="{Binding S1}" x:Name="t0" HorizontalAlignment="Left" Margin="135,55,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="21" Width="231"/>
        <TextBlock Text="{Binding S2}" x:Name="t1" HorizontalAlignment="Left" Margin="135,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="21" RenderTransformOrigin="0.5,0.5" Width="234"/>
        <TextBlock Text="{Binding S3}" x:Name="t2" HorizontalAlignment="Left" Margin="135,194,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="21" Width="231"/>
        <TextBlock Text="{Binding S4}" x:Name="t3" HorizontalAlignment="Left" Margin="135,264,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="21" Width="231"/>
        <Label x:Name="label4" Content="Renoviranje" HorizontalAlignment="Left" Margin="50,320,0,0" VerticalAlignment="Top"/>
        <TextBlock Text="{Binding S5}" x:Name="t4" HorizontalAlignment="Left" Margin="135,324,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="21" Width="231"/>
    </Grid>
</Page>
