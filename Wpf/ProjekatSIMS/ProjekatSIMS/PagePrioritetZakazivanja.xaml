<Page x:Class="ProjekatSIMS.PagePrioritetZakazivanja"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ProjekatSIMS"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="PagePrioritetZakazivanja" Width="1272" Height="649" Background="LightBlue">

    <Grid>
        <Label x:Name="label" Content="Izaberite prioritet zakazivanja" HorizontalAlignment="Left" Margin="384,95,0,0" VerticalAlignment="Top" Style="{StaticResource ResourceKey=TitleStyle}"/>
        <RadioButton x:Name="radioButton" Content="Datum" HorizontalAlignment="Left" Margin="548,250,0,0" VerticalAlignment="Top" FontSize="36" Checked="Datum_Checked"/>
        <RadioButton x:Name="radioButton1" Content="Lekar" HorizontalAlignment="Left" Margin="548,340,0,0" VerticalAlignment="Top" FontSize="36" Checked="Lekar_Checked"/>
        <Button x:Name="button" Content="Nazad" HorizontalAlignment="Left" Margin="548,484,0,0" VerticalAlignment="Top" Click="Nazad_Click" Style="{StaticResource ResourceKey=ButtonStyle}"/>

    </Grid>
</Page>
